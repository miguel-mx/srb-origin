{# src/Ccm/SrbBundle/Resources/views/Search/query.html.twig #}
{% extends 'CcmSrbBundle::layout.html.twig' %}

{% block menu %}
{{ knp_menu_render('CcmSrbBundle:Builder:mainMenu') }}
{% endblock %}
{% set option = 'Buscar publicación' %}


{% block col3 %}

<h1>Búsqueda Bibliográfica {{ msg }}</h1>
<br><br>

<form action="{{ path('search') }}" method="post" {{ form_enctype(form) }}>

    {{ form_errors(form) }}

    {{ form_widget(form) }}

    {{ form_rest(form) }}
    <br>
    <button type="submit" class="ym-button ym-search" /> Buscar</button>

</form>

{% endblock %}

{% block menuL %}


<ul class="record_actions">
    <li>
        <a href="{{ path('refs') }}">
            Regresar
        </a>
    </li>
   
    
</ul>
{% endblock %}
