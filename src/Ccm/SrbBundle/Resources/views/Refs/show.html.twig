	  <div class="ym-g80 ym-gl">
	    <div style="padding:3px;"> <b>Author: </b> {% for key in entity.author|explode %}<a href="{{key}}"> {{key|trim}}</a>   {% endfor %} </div>

    
	  </div>
	{% endif %}
	   
